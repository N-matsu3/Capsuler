 <%= form_with model: @item, url: item_path(@item), method: :patch do |f| %>
 <table class="table border-borderless">
          <tr>
            <th>イメージ</th><td><%= f.file_field :image, placeholder: "ガチャ画像", accept: "image/*" %></td>
          </tr>
           <tr>
            <th>タイトル</th><td> <%= f.text_field :title %></td>
          </tr>
           <tr>
            <th>アイテム詳細</th><td><%= f.text_area :detail %></td>
          </tr>
          </tr>
           <tr>
            <th>タグ</th><td></td>
          </tr>
          </tr>
           <tr>
            <th>位置情報</th><td></td>
          </tr>
          <tr>
            <th></th><td><%= f.submit '編集保存',class:'btn btn-success w-50' %></td>
          </tr>
 </table>
 <% end %>