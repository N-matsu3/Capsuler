自分の投稿一覧
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h3><%= @user.user_name %>さんの投稿一覧</h3>


          <% @items.each do |item| %>

          <% if item.image.attached? %>
            <%=image_tag item.image, size: "200x200" %>
          <% else %>
            <%= image_tag 'no_image', size: "200x200" %>
          <% end %>


          <%= link_to item_path(item) do %>
            <%= item.title %>
          <% end %>
          <% end %>



    </div>
  </div>
</div>